<%= turbo_frame_tag dom_id(uploadlink.topic) do %>
<div class="d-flex justify-content-between">
  <%= link_to "#{uploadlink.topic.name}", edit_topic_path(uploadlink.topic),
              class: "list-group-item list-group-item-action",
              id: "#{uploadlink.topic.name}-edit-button" %>
  <%= link_to "x", uploadlink, method: :delete,
              class: "btn btn-outline-danger",
              id: "#{uploadlink.topic.name}-delete-button" %>
</div>
<% end %>